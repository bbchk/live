{
  "git.ignoreLimitWarning": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "    **/*.css": true,
    "    **/*.module.scss": true,
    "    **/*.scss": true,
    "**/*.css.map": true,
    "**/*.gitignore": true,
    "**/*.md": true,
    "**/*.module.css": true,
    "front/tsconfig.json": true,
    "front/next.config.js": true,
    "front/.eslintrc.json": true,
    "front/next-env.d.ts": true,
    "front/package-lock.json": true,
    "front/node_modules": true,
    "front/.next": true,
    "**/.vscode": true,
    "back/node_modules": true,
    "back/package-lock.json": true,
    "back/.gitignore": true
  },
  "explorerExclude.backup": {},
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0e4c0c",
    "activityBar.background": "#0e4c0c",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#5a5ee5",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0e4c0c",
    "statusBar.background": "#062005",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0e4c0c",
    "statusBarItem.remoteBackground": "#062005",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#062005",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#06200599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#062005"
}
